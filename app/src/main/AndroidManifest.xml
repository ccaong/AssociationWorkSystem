<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gqsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <!--添加联系人-->
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.USE_BIOMETRIC" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity
            android:name=".ui.activity.splash.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.activity.login.LoginActivity" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".ui.projectlist.dualprevention.DoublePreventionActivity" />
        <activity android:name=".ui.projectlist.evaluate.EvaluateActivity" />
        <activity android:name=".ui.projectlist.securitymanagement.SecurityManagementActivity" />
        <activity android:name=".ui.projectlist.informationize.InformationizeActivity" />
        <activity android:name=".ui.projectlist.govproject.GovProjectActivity" />
        <activity android:name=".ui.projectlist.specialrectification.SpecialRectificationActivity" />
        <activity android:name=".ui.activity.lock.LockActivity" />
    </application>

</manifest>